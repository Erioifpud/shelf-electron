{
  "compilerOptions": {
    "target": "ES2024",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "lib": ["ESNext", "DOM"],
    "outDir": "dist",
    "strict": true,
    "skipLibCheck": true,
    "verbatimModuleSyntax": true,
    "baseUrl": ".",
    //"composite": true,
    "declaration": true,
    "declarationMap": true,
    "emitDeclarationOnly": false,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": ["node_modules"]
}